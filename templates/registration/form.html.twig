{% extends 'base.html.twig' %}

{% block title 'actor.registration'|trans %}

{% block body %}
    {{ form_start(form) }}
        {{ form_errors(form) }}
        {{ form_row(form.firstName) }}
        {{ form_row(form.lastName) }}
        {{ form_row(form.birthday) }}
        {{ form_row(form.emailAddress) }}
        {{ form_row(form.password) }}

        <button type="submit">
            {{ 'actor.registration.register'|trans }}
        </button>
    {{ form_end(form) }}
{% endblock %}
